<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Disable App Sandbox for full Keychain and git access -->
    <!-- Note: This means the app cannot be distributed on the Mac App Store -->
    <!-- For App Store distribution, enable sandbox and use keychain-access-groups -->
    <key>com.apple.security.app-sandbox</key>
    <false/>

    <!-- Allow unsigned executable memory (for some git operations) -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <false/>

    <!-- Disable library validation -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <false/>
</dict>
</plist>
